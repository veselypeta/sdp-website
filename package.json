{
  "name": "sdp-website",
  "version": "1.0.0",
  "description": "### Prerequisites - node  -   12.10.0 or greater - npm   -   6.10.0 or greater",
  "main": "index.js",
  "scripts": {
    "start": "tsc --esModuleInterop && node ./build/server/app.js",
    "start:watch": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && cd ./client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veselypeta/sdp-website.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/veselypeta/sdp-website/issues"
  },
  "homepage": "https://github.com/veselypeta/sdp-website#readme",
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^13.9.5",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "server"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
