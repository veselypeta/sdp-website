{
  "name": "omniopener",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.30",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "bootstrap": "^4.4.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "compile": "tsc -p tsconfig.json --noEmit",
    "start": "react-scripts start",
    "build": "react-scripts build && rm -rf ../build/client && mv -f ./build ../build/client",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0 --no-ignore",
    "lint:fix": "npm run lint -- --fix",
    "test:coverage": "npm test -- --watchAll=false --coverage --ci --runInBand"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run compile && npm run lint && npm run test:coverage",
      "pre-push": "npm run compile && npm run lint && npm run test:coverage"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "prettier": "^2.0.1",
    "tslint": "^6.1.0"
  },
  "proxy": "http://localhost:5000/"
}
